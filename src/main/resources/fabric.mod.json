{
  "schemaVersion": 1,
  "id": "gelatin",
  "version": "${version}",

  "name": "Gelatin",
  "description": "A Library Mod full of colorful tools to make it easier to Color Minecraft's various colorful blocks",
  "authors": [
    "Blodhgarm"
  ],
  "contact": {
    "homepage": "https://github.com/wisp-forest/gelatin",
    "issues": "https://github.com/wisp-forest/gelatin/issues",
    "sources": "https://github.com/wisp-forest/gelatin"
  },

  "license": "MIT",
  "icon": "assets/gelatin/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.wispforest.gelatin.Gelatin"
    ],
    "preLaunch": [
      "io.wispforest.gelatin.Gelatin"
    ]
  },

  "depends": {
    "fabricloader": ">=0.14.21",
    "fabric": "*",
    "minecraft": "~1.20",
    "java": ">=17"
  },

  "recommends": {
    "owo": "*"
  },
  "custom": {
    "gelatin": {
      "icon-redirect": "gelatin-common:assets/gelatin/icon.png"
    }
  }
}
