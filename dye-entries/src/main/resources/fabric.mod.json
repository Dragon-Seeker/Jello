{
  "schemaVersion": 1,
  "id": "dye_block_and_item",
  "version": "${version}",

  "name": "Dye Blocks And Items",
  "description": "Main Module within Gelatin which contains the bulk code for Custom Block and Item Colors",
  "authors": [
    "blodhgarm"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/gelatin/icon.png",
  "accessWidener" : "dye-entries.accesswidener",

  "environment": "*",
  "entrypoints": {
    "main": [
      "io.wispforest.dye_entries.DyeEntriesInit"
    ],
    "client": [
      "io.wispforest.dye_entries.client.DyeEntriesClientInit"
    ],
    "condensed_creative": [
      "io.wispforest.dye_entries.compat.condensedCreative.JelloCCEntrypoint"
    ],
    "rei_client": [
      "io.wispforest.dye_entries.compat.rei.GelatinREIClientPlugin"
    ]
  },
  "mixins": [
    "dye-entries.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  },
  "custom": {
    "modmenu": {
      "badges": [ "library" ],
      "parent": {
        "id": "gelatin",
        "name": "Gelatin",
        "description": "A API full of Random Colorful stuff",
        "icon": "assets/gelatin/icon.png",
        "badges": [ "library" ]
      }
    }
  }
}
